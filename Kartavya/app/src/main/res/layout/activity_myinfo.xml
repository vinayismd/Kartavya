<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.app.MainActivity$PlaceholderFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            >


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Name"
                android:id="@+id/textView1"
                android:layout_marginTop="20dp"
                android:paddingBottom="5dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Name"
                android:id="@+id/tvmyinfoname"
                android:paddingBottom="15dp"
                android:layout_below="@+id/textView1"
                android:layout_marginLeft="100dp"
                 />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Admission No"
                android:id="@+id/textView3"
                android:paddingBottom="5dp"
                android:layout_below="@+id/tvmyinfoname"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Admission no"
                android:paddingBottom="15dp"
                android:id="@+id/tvmyinfoadmno"
                android:layout_below="@+id/textView3"
                android:layout_marginLeft="100dp"
                 />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Password"
                android:paddingBottom="5dp"
                android:id="@+id/textView5"
                android:layout_below="@+id/tvmyinfoadmno"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Password"
                android:paddingBottom="15dp"
                android:id="@+id/tvmyinfopass"
                android:layout_below="@+id/textView5"
                android:layout_marginLeft="100dp"
                 />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Year"
                android:paddingBottom="5dp"
                android:id="@+id/textView7"
                android:layout_below="@+id/tvmyinfopass"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="year"
                android:paddingBottom="15dp"
                android:id="@+id/tvmyinfoyear"
                android:layout_below="@+id/textView7"
                android:layout_marginLeft="100dp"
                 />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Branch"
                android:paddingBottom="5dp"
                android:id="@+id/textView9"
                android:layout_below="@+id/tvmyinfoyear"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Branch"
                android:paddingBottom="15dp"
                android:id="@+id/tvmyinfobranch"
                android:layout_below="@+id/textView9"
                android:layout_marginLeft="100dp"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Centre"
                android:paddingBottom="5dp"
                android:id="@+id/textView11"
                android:layout_below="@+id/tvmyinfobranch"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Centre"
                android:paddingBottom="15dp"
                android:id="@+id/tvmyinfocentre"
                android:layout_below="@+id/textView11"
                android:layout_marginLeft="100dp"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Team"
                android:paddingBottom="5dp"
                android:id="@+id/textView13"
                android:layout_below="@+id/tvmyinfocentre"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="team"
                android:paddingBottom="15dp"
                android:id="@+id/tvmyinfoteam"
                android:layout_below="@+id/textView13"
                android:layout_marginLeft="100dp"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Working Under"
                android:paddingBottom="5dp"
                android:id="@+id/textView15"
                android:layout_below="@+id/tvmyinfoteam"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Working under"
                android:paddingBottom="15dp"
                android:id="@+id/tvmyinfowrkun"
                android:layout_below="@+id/textView15"
                android:layout_marginLeft="100dp"
               />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Phone Number"
                android:paddingBottom="5dp"
                android:id="@+id/textView17"
                android:layout_below="@+id/tvmyinfowrkun"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Phone number"
                android:paddingBottom="15dp"
                android:id="@+id/tvmyinfophnno"
                android:layout_below="@+id/textView17"
                android:layout_marginLeft="100dp"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Email Address"
                android:paddingBottom="5dp"
                android:id="@+id/textView19"
                android:layout_below="@+id/tvmyinfophnno"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Email"
                android:paddingBottom="5dp"
                android:id="@+id/tvmyinfoemail"
                android:layout_below="@+id/textView19"
                android:layout_marginLeft="100dp"
                 />
        </RelativeLayout>
    </ScrollView>


</RelativeLayout>